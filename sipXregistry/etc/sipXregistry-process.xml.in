<?xml version='1.0' encoding='iso-8859-1' standalone='yes'?>
<sipXecs-process>
  <name>SIPRegistrar</name>
  <version>@VERSION@</version>
  <commands>
    <configtest>
      <execute>@SIPX_BINDIR@/sipregistrar.sh</execute>
      <parameter>--configtest</parameter>
    </configtest>
    <start>
      <execute>@SIPX_BINDIR@/sipregistrar.sh</execute>
    </start>
    <stop>
      <execute>@SIPX_BINDIR@/sipregistrar.sh</execute>
      <parameter>--stop</parameter>
    </stop>
  </commands>
  <status isService='true'>
    <pid>@SIPX_RUNDIR@/sipregistrar.pid</pid>
    <log>@SIPX_LOGDIR@/sipregistrar.log</log>
  </status>
  <resources>
    <osconfig>@SIPX_CONFDIR@/domain-config</osconfig>
    <osconfig>@SIPX_CONFDIR@/registrar-config</osconfig>
    <file>@SIPX_CONFDIR@/mappingrules.xml</file>
    <file>@SIPX_CONFDIR@/fallbackrules.xml</file>
    <imdb>credential</imdb>
    <imdb>permission</imdb>
    <imdb>alias</imdb>
    <imdb>huntgroup</imdb>
    <imdb>registration</imdb>
  </resources>
</sipXecs-process>
