    <setting name="SNTPEnable">
      <label>Enable SNTP</label>
      <type>
        <boolean />
      </type>
      <value>0</value>
    </setting>
    <setting name='SNTP1stServ'>
      <value />
    </setting>
    <setting name='SNTP2ndServ'>
      <value />
    </setting>
    <setting name='SNTPTimeZone'>
      <type>
        <enum>
          <option>
            <label>-12:00</label>
            <value>-12:00</value>
          </option>
          <option>
            <label>-11:00</label>
            <value>-11:00</value>
          </option>
          <option>
            <label>-10:00</label>
            <value>-10:00</value>
          </option>
