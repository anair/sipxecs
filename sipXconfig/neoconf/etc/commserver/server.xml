<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE model 
    PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN" 
    "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd">
<model>
  <type id="loglevel">
    <enum>
      <option>
        <value>DEBUG</value>
      </option>
      <option>
        <value>INFO</value>
      </option>
      <option>
        <value>NOTICE</value>
      </option>
      <option>
        <value>WARNING</value>
      </option>
      <option>
        <value>ERR</value>
      </option>
      <option>
        <value>CRIT</value>
      </option>
      <option>
        <value>ALERT</value>
      </option>
      <option>
        <value>EMERG</value>
      </option>
    </enum>
  </type>
  <type id="enablestate">
    <boolean>
      <true>
        <value>ENABLE</value>
      </true>
      <false>
        <value>DISABLE</value>
      </false>
    </boolean>
  </type>
  <type id="port-number">
    <integer required="yes" min="1024" max="65534" />
  </type>
  <group name="domain">
    <setting name="SIPXCHANGE_DOMAIN_NAME" hidden="yes">
      <profileName>config.defs</profileName>
      <type />
      <value />
    </setting>
  </group>
  <group name="voice-mail">
    <setting name="VOICEMAIL_AGE_LIMIT">
      <profileName>config.defs</profileName>
      <type>
        <integer min="1" max="365" required="yes" />
      </type>
      <value>7</value>
    </setting>
    <setting name="SMTP_SERVER" hidden="yes">
      <profileName>config.defs</profileName>
      <type />
      <value />
    </setting>
  </group>
  <group name="Logging">
    <setting name="SIP_CALLRESOLVER_LOG_LEVEL">
      <profileName>callresolver-config.in</profileName>
      <type refid="loglevel" />
      <value>NOTICE</value>
    </setting>
  </group>
  <group name="cdr">
    <setting name="CALLRESOLVER_CALL_STATE_DB">
      <profileName>config.defs</profileName>
      <type refid="enablestate" />
      <value>ENABLE</value>
    </setting>
    <setting name="SIP_CALLRESOLVER_PURGE">
      <profileName>callresolver-config.in</profileName>
      <type refid="enablestate" />
      <value>ENABLE</value>
    </setting>
    <setting name="SIP_CALLRESOLVER_PURGE_AGE_CDR">
      <profileName>callresolver-config.in</profileName>
      <type>
        <integer min="1" max="365" />
      </type>
      <value>35</value>
    </setting>
    <setting name="SIP_CALLRESOLVER_PURGE_AGE_CSE" advanced="yes">
      <profileName>callresolver-config.in</profileName>
      <type>
        <integer min="1" max="365" />
      </type>
      <value>7</value>
    </setting>
    <setting name="SIP_CALLRESOLVER_CALL_DIRECTION" hidden="yes">
      <profileName>callresolver-config.in</profileName>
      <type refid="enablestate" />
      <value>DISABLE</value>
    </setting>
  </group>
</model>
