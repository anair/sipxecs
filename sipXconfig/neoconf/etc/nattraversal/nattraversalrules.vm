<?xml version="1.0" encoding="UTF-8"?>

<!-- DO NOT EDIT THIS FILE. It is generated by sipXconfig web ui -->
<nattraversal xmlns="http://www.sipfoundry.org/sipX/schema/xml/nattraversalrules-00-00">
  <info>
	<state>${state}</state>
	<behindnat>${behindnat}</behindnat>
#if(${behindnat})
    <publicaddress>${publicaddress}</publicaddress>
#end
	<proxyhostport>${dollar}{MY_IP_ADDR}:${dollar}{PROXY_SERVER_SIP_PORT}</proxyhostport>
	<relayaggressiveness>${relayaggressiveness}</relayaggressiveness>
	<concurrentrelays>${maxconcurrentrelays}</concurrentrelays>
#if(${bridge})
	<mediarelaypublicaddress>${mediarelaypublicaddress}</mediarelaypublicaddress>
	<mediarelaynativeaddress>${mediarelaynativeaddress}</mediarelaynativeaddress>
	<mediarelayxml-rpc-port>${mediarelayxml-rpc-port}</mediarelayxml-rpc-port>
#end
  </info>
  <localtopology>
    #foreach($subnet in ${sbc.routes.subnets})
      <ipV4subnet>$subnet</ipV4subnet>
    #end
    #foreach($domain in ${sbc.routes.domains})
      <dnsWildcard>$domain</dnsWildcard>
    #end
  </localtopology>
</nattraversal>
