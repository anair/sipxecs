<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE model 
    PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN" 
    "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd">
<model>
  <type id="loglevel">
    <enum>
      <option>
        <value>DEBUG</value>
      </option>
      <option>
        <value>INFO</value>
      </option>
      <option>
        <value>NOTICE</value>
      </option>
      <option>
        <value>WARNING</value>
      </option>
      <option>
        <value>ERR</value>
      </option>
      <option>
        <value>CRIT</value>
      </option>
      <option>
        <value>ALERT</value>
      </option>
      <option>
        <value>EMERG</value>
      </option>
    </enum>
  </type>
  <type id="enablestate">
    <boolean>
      <true>
        <value>ENABLE</value>
      </true>
      <false>
        <value>DISABLE</value>
      </false>
    </boolean>
  </type>
  <type id="feature-code">
    <string>
      <pattern>[0-9*#]+</pattern>
    </string>
  </type>
  <type id="port-number">
    <integer required="yes" min="1024" max="65534" />
  </type>
  <group name="domain">
    <setting name="SIPXCHANGE_DOMAIN_NAME" hidden="yes">
      <profileName>config.defs</profileName>
      <type />
      <value />
    </setting>
  </group>
  <group name="voice-mail">
    <setting name="VOICEMAIL_AGE_LIMIT">
      <profileName>config.defs</profileName>
      <type>
        <integer min="1" max="365" required="yes" />
      </type>
      <value>7</value>
    </setting>
    <setting name="SMTP_SERVER" hidden="yes">
      <profileName>config.defs</profileName>
      <type />
      <value />
    </setting>
  </group>
  <group name="presence">
    <setting name="SIP_PRESENCE_SIGN_IN_CODE">
      <profileName>sipxpresence-config.in</profileName>
      <type refid="feature-code" />
      <value>*88</value>
    </setting>
    <setting name="SIP_PRESENCE_SIGN_OUT_CODE">
      <profileName>sipxpresence-config.in</profileName>
      <type refid="feature-code" />
      <value>*86</value>
    </setting>
    <setting name="PRESENCE_SERVER_SIP_PORT" advanced="yes">
      <profileName>config.defs</profileName>
      <type refid="port-number" />
      <value>5130</value>
    </setting>
    <setting name="PRESENCE_SERVER_HTTP_PORT" advanced="yes">
      <profileName>config.defs</profileName>
      <type refid="port-number" />
      <value>8111</value>
    </setting>
    <setting name="SIP_PRESENCE_DOMAIN_NAME" hidden="yes">
      <!-- treat as read-only: we are taking this from sipxpresence-config and not from sipxpresence-config.in -->
      <profileName>sipxpresence-config</profileName>
      <value />
    </setting>
    <setting name="SIP_PRESENCE_HTTP_PORT" hidden="yes">
      <!-- treat as read-only: we are taking this from sipxpresence-config and not from sipxpresence-config.in -->
      <profileName>sipxpresence-config</profileName>
      <type refid="port-number" />
      <value>8111</value>
    </setting>
  </group>
  <group name="Logging">
    <setting name="SIP_PARK_LOG_LEVEL">
      <profileName>sipxpark-config.in</profileName>
      <type refid="loglevel" />
      <value>NOTICE</value>
    </setting>
    <setting name="SIP_STATUS_LOG_LEVEL">
      <profileName>status-config.in</profileName>
      <type refid="loglevel" />
      <value>NOTICE</value>
    </setting>
    <setting name="SIP_PRESENCE_LOG_LEVEL">
      <profileName>sipxpresence-config.in</profileName>
      <type refid="loglevel" />
      <value>NOTICE</value>
    </setting>
    <setting name="SIP_CALLRESOLVER_LOG_LEVEL">
      <profileName>callresolver-config.in</profileName>
      <type refid="loglevel" />
      <value>NOTICE</value>
    </setting>
    <setting name="SIP_RLS_LOG_LEVEL">
      <profileName>sipxrls-config.in</profileName>
      <type refid="loglevel" />
      <value>NOTICE</value>
    </setting>
    <setting name="log.level">
      <profileName>sipxpage.properties.in</profileName>
      <type refid="loglevel" />
      <value>NOTICE</value>
    </setting>
  </group>
  <group name="cdr">
    <setting name="CALLRESOLVER_CALL_STATE_DB">
      <profileName>config.defs</profileName>
      <type refid="enablestate" />
      <value>ENABLE</value>
    </setting>
    <setting name="SIP_CALLRESOLVER_PURGE">
      <profileName>callresolver-config.in</profileName>
      <type refid="enablestate" />
      <value>ENABLE</value>
    </setting>
    <setting name="SIP_CALLRESOLVER_PURGE_AGE_CDR">
      <profileName>callresolver-config.in</profileName>
      <type>
        <integer min="1" max="365" />
      </type>
      <value>35</value>
    </setting>
    <setting name="SIP_CALLRESOLVER_PURGE_AGE_CSE" advanced="yes">
      <profileName>callresolver-config.in</profileName>
      <type>
        <integer min="1" max="365" />
      </type>
      <value>7</value>
    </setting>
    <setting name="SIP_CALLRESOLVER_CALL_DIRECTION" hidden="yes">
      <profileName>callresolver-config.in</profileName>
      <type refid="enablestate" />
      <value>DISABLE</value>
    </setting>
  </group>
  <group name="park" hidden="yes">
    <setting name="SIP_REGISTRAR_PARK_SERVER">
      <profileName>registrar-config</profileName>
    </setting>
    <setting name="SIP_PARK_DOMAIN_NAME">
      <profileName>sipxpark-config</profileName>
    </setting>
    <setting name="SIP_PARK_UDP_PORT">
      <profileName>sipxpark-config</profileName>
      <type refid="port-number" />
    </setting>
  </group>
</model>
