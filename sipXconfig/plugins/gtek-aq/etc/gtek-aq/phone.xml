<?xml version="1.0"?>
<!DOCTYPE model PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN"
  "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd">
<model>
  <group name="AutoConfigVersion" hidden="yes">
    <label>Auto Config Version</label>
    <profileName>Auto Config Version</profileName>
    <setting name="AutoConfigVersion">
      <label>Version</label>
      <type>
        <integer />
      </type>
    </setting>
  </group>
  <group name="PhoneForward" hidden="yes">
    <label>Phone Forward</label>
    <profileName>Phone Forward</profileName>
    <setting name="ph1AllFwd">
      <label>Enable All Forward</label>
      <type>
        <boolean />
      </type>
      <value>0</value>
    </setting>
    <setting name="ph1BusyFwd">
      <label>Enable Busy Forward</label>
      <type>
        <boolean />
      </type>
      <value>0</value>
    </setting>
    <setting name="ph1NoAnsFwd">
      <label>Enable No Answer Forward</label>
      <type>
        <boolean />
      </type>
      <value>0</value>
    </setting>
    <setting name="ph1AllFwdName">
      <label>All Forward Name</label>
      <type>
        <string />
      </type>
    </setting>
    <setting name="ph1AllFwdUrl">
      <label>All Forward URL</label>
      <type>
        <string />
      </type>
    </setting>
    <setting name="ph1BusyFwdName">
      <label>Busy Forward Name</label>
      <type>
        <string />
      </type>
    </setting>
    <setting name="ph1BusyFwdUrl">
      <label>Busy Forward URL</label>
      <type>
        <string />
      </type>
    </setting>
    <setting name="ph1NoAnsFwdName">
      <label>No Answer Forward Name</label>
      <type>
        <string />
      </type>
    </setting>
    <setting name="ph1NoAnsFwdUrl">
      <label>No Answer Forward URL</label>
      <type>
        <string />
      </type>
    </setting>
    <setting name="ph1NoAnsTimeOut">
      <label>No Answer Ring Count</label>
      <type>
        <integer min="2" max="8" />
      </type>
      <value>4</value>
    </setting>
  </group>
  <group name="MoHSettings">
    <label>Music on Hold</label>
    <profileName>MoH Settings</profileName>
    <setting name="ph1HoldRFCEnable">
      <label>Enable MoH</label>
      <type>
        <boolean />
      </type>
      <value>1</value>
    </setting>
    <setting name="NMOH" advanced="yes">
      <label>MoH URL</label>
      <type>
        <string />
      </type>
    </setting>
  </group>
  <group name='SNTP_Settings'>
