<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" 
	"http://www.springframework.org/dtd/spring-beans.dtd">
<beans>

  <bean id="phonebookManager" class="org.springframework.aop.framework.ProxyFactoryBean"
    parent="abstractDao">
    <property name="proxyInterfaces">
      <value>org.sipfoundry.sipxconfig.phonebook.PhonebookManager</value>
    </property>
    <property name="target">
      <ref local="phonebookManagerImpl" />
    </property>
  </bean>

  <bean id="phonebookManagerImpl"
    class="org.sipfoundry.sipxconfig.phonebook.PhonebookManagerImpl">
    <property name="sessionFactory" ref="sessionFactory"/>
    <property name="coreContext" ref="coreContext"/>
    <property name="phonebookManagementEnabled" value="true" />
	<property name="telType" value="work" />
  </bean>

</beans>
