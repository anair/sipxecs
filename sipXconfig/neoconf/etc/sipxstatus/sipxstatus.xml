<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE model 
    PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN" 
    "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd">
<model>
  <type id="loglevel">
    <enum>
      <option>
        <value>DEBUG</value>
      </option>
      <option>
        <value>INFO</value>
      </option>
      <option>
        <value>NOTICE</value>
      </option>
      <option>
        <value>WARNING</value>
      </option>
      <option>
        <value>ERR</value>
      </option>
      <option>
        <value>CRIT</value>
      </option>
      <option>
        <value>ALERT</value>
      </option>
      <option>
        <value>EMERG</value>
      </option>
    </enum>
  </type>
  <group name="status-config">
    <setting name="SIP_STATUS_LOG_LEVEL">
      <type refid="loglevel" />
      <value>NOTICE</value>
    </setting>
    <setting name="SIP_STATUS_HTTP_PORT" hidden="yes">
      <type>
        <string />
      </type>
      <value>8100</value>
    </setting>
    <setting name="SIP_STATUS_SIP_PORT" hidden="yes">
      <type>
        <string />
      </type>
      <value>5110</value>
    </setting>
  </group>
</model>
