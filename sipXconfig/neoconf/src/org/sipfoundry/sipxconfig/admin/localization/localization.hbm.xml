<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.sipfoundry.sipxconfig.admin.localization" default-lazy="false">
  <class name="Localization" table="localization">
    <id name="id" column="localization_id" type="int" unsaved-value="-1">
      <generator class="sequence">
        <param name="sequence">localization_seq</param>
      </generator>
    </id>
    <property name="region" />
    <property name="language" />
  </class>

</hibernate-mapping>
