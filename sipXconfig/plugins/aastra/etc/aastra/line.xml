<?xml version='1.0' encoding='UTF-8'?>

<!DOCTYPE model PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN" "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd"[
<!ENTITY types SYSTEM "types.xml">
]>

<model>
  <group name='sip_id'>
    <setting name='screen_name'>
      <type>
        <string/>
      </type>
      <value/>
    <profileName>screen name</profileName>
    </setting>
    <setting name='screen_name2'>
      <type>
        <string/>
      </type>
      <value/>
    <profileName>screen name 2</profileName>
    </setting>
    <setting name='user_name'>
      <type>
        <string/>
      </type>
      <value/>
    <profileName>user name</profileName>
    </setting>
    <setting name='display_name'>
      <type>
        <string/>
      </type>
      <value/>
    <profileName>display name</profileName>
    </setting>
    <setting name='auth_name'>
      <type>
        <string/>
      </type>
      <value/>
    <profileName>auth name</profileName>
    </setting>
    <setting name='password'>
      <type>
        <string/>
      </type>
      <value/>
    <profileName>password</profileName>
    </setting>
  </group>

  <group name="server">
    <setting name='proxyIp'>
      <type/>
      <value>0.0.0.0</value>
    <profileName>proxy ip</profileName>
    </setting>
    <setting name='proxy_port' advanced="yes">
      <type>
        <integer/>
      </type>
      <value>0</value>
    <profileName>proxy port</profileName>
    </setting>
    <setting name='registrar_ip'>
      <type/>
      <value>0.0.0.0</value>
    <profileName>registrar ip</profileName>
    </setting>
    <setting name='registrar_port' advanced="yes">
      <type>
        <integer/>
      </type>
      <value>0</value>
    <profileName>registrar port</profileName>
    </setting>
    <setting name='outbound_proxy' advanced="yes">
      <type/>
      <value>0.0.0.0</value>
    <profileName>outbound proxy</profileName>
    </setting>
    <setting name='outbound_proxy_port' advanced="yes">
      <type>
        <integer/>
      </type>
      <value>0</value>
    <profileName>outbound proxy port</profileName>
    </setting>
    <setting name='centralized_conf'>
      <type>
        <string/>
      </type>
      <value/>
    <profileName>centralized conf</profileName>
    </setting>
  </group>
  <group name="Settings">
    <setting name='registration_period' advanced="yes">
      <type>
        <integer/>
      </type>
      <value>0</value>
    <profileName>registration period</profileName>
    </setting>
    <setting name='dtmf_method'>
      <type>
        <integer/>
      </type>
      <value>0</value>
    <profileName>dtmf method</profileName>
    </setting>
    <setting name='srtp_mode'>
      <type>
        <integer max='2' min='-1'/>
      </type>
      <value>0</value>
    <profileName>srtp mode</profileName>
    </setting>
  </group>

  <group name="Not_needed">
    <setting name='bla_number'>
      <type>
        <integer/>
      </type>
      <value/>
    <profileName>bla number</profileName>
    </setting>
    <setting name='line_summary_subscription'>
      <type>
        <boolean/>
      </type>
      <value>0</value>
    <profileName>line summary subscription</profileName>
    </setting>
    <setting name='mode'>
      <type>
        <integer/>
      </type>
      <value/>
    <profileName>mode</profileName>
    </setting>
  </group>
</model>

