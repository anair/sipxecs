
# G E N E R A L
version=@VERSION@
top.dir=@abs_top_srcdir@
src.dir=@abs_srcdir@
java=@JAVA@
java.home=@JAVA_HOME@
jdk.include=@JAVA_HOME@/include
optimize=@JAVAC_OPTIMIZED@
deprecation=false
debug=@JAVAC_DEBUG@
bin.dir=@bindir@
lib.dir=@libdir@
prefix.dir=@prefix@
conf.dir=@SIPX_CONFDIR@

# comma-separated combination of 'lines' 'source' and 'var' only valid
# when debug is on
debuglevel=lines

# Destination directory for runtime files.
sipxcommons.dir=@SIPX_JAVADIR@/sipXcommons

# Third parth JAR files.
cpptasks.jar=${src.dir}/lib/cpptasks-1.0b4.jar
JainSip.jar=${src.dir}/lib/jain-sip-sdp-1.2.74.jar
junit.jar=${src.dir}/lib/junit-4.4.jar
log4j.jar=${src.dir}/lib/log4j-1.2.15.jar
dnsjava.jar=${src.dir}/lib/dnsjava-2.0.6.jar
