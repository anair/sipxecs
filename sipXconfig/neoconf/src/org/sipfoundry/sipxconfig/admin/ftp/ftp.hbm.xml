<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.sipfoundry.sipxconfig.admin.ftp" default-lazy="false">
  <!--
    - FTP Configuration
  -->
  <class name="FtpConfiguration" table="ftp_configuration">
    <id name="id" column="id" type="int" unsaved-value="-1">
      <generator class="sequence">
        <param name="sequence">ftp_configuration_seq</param>
      </generator>
    </id>
    <property name="host" />
    <property name="userId" />
    <property name="password" />
  </class>
</hibernate-mapping>